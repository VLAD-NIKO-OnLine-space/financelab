  "dependencies": {
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "json-server": "^0.17.4",
    "mobx": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lineto": "^3.3.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "react-vis": "^1.12.1",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
